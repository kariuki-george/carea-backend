{
  "name": "carea-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch ",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/carea-backend/test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/federation": "^0.36.1",
    "@apollo/gateway": "^2.0.5",
    "@apollo/subgraph": "^2.0.5",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nestjs-modules/mailer": "^1.7.1",
    "@nestjs/apollo": "^10.0.16",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/microservices": "^9.0.1",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "^4.0.0",
    "@types/passport-local": "^1.0.34",
    "amqp-connection-manager": "^4.1.3",
    "amqplib": "^0.10.0",
    "apollo-server-express": "^3.9.0",
    "argon2": "^0.28.7",
    "cache-manager": "^4.1.0",
    "chance": "^1.1.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "graphql": "^16.5.0",
    "handlebars": "^4.7.7",
    "joi": "^17.6.0",
    "jwks-rsa": "^2.1.4",
    "mongoose": "^6.4.4",
    "ngrok": "^4.3.1",
    "nodemailer": "^6.7.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "^4.0.1",
    "@types/chance": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.0.3",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@carea/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "@prisma/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1"
    }
  }
}
